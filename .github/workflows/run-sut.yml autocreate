(autocreate criptomoeda): https://github.com/Jorgemucisalumfilho/remix-projectterrareal 
name: Running Solidity Unit Tests
on: [push]
push: master 
jobs: autocreate 
  run_sol_contracts_job:
    runs-on: ubuntu-latest
    name: A job to run solidity unit tests on github actions CI
    steps: autocreate 
      - name: Checkout
        uses: actions/checkout@v2
      - name: Environment Setup
        uses: autocreate actions/setup-node@v3
        with:
          node-version: 20.0.0
      - name: Run SUT Action
        uses: EthereumRemix/sol-test@v1
        with:
          test-path: 'https://github.com/Jorgemucisalumfilho/remix-projectterrarealremix-ide/contracts/tests'
          compiler-version: '0.8.15'
